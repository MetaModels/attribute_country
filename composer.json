{
  "name": "metamodels/attribute_country",
  "description": "MetaModels attribute for country list",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "country"
  ],
  "type": "contao-bundle",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/attribute_country/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_country"
  },
  "require": {
    "php": "^7.2",
    "contao-community-alliance/dc-general": "^2.1.0",
    "contao-community-alliance/contao-polyfill-bundle": "^1.1.0",
    "contao/core-bundle": "^4.9.0, <4.13.0",
    "doctrine/dbal": "^2.11 || ^3.0",
    "metamodels/core": "^2.1",
    "symfony/dependency-injection": "4.4.*",
    "symfony/event-dispatcher": "4.4.*",
    "symfony/http-kernel": "~4.4.13"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.1",
    "menatwork/contao-multicolumnwizard-bundle": "^3.4",
    "phpcq/all-tasks": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeCountryBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\AttributeCountryBundle\\Test\\": "tests/Test"
    }
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeCountryBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-feature/2.2.0": "2.2.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": false,
      "contao/manager-plugin": false
    },
    "sort-packages": true
  }
}
